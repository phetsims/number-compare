{
  "name": "number-play",
  "version": "1.1.0-dev.0",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/number-play.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "NUMBER_PLAY",
    "phetLibs": [
      "counting-common",
      "fractions-common",
      "twixt",
      "vegas"
    ],
    "simFeatures": {
      "supportsSound": true
    },
    "simulation": true,
    "runnable": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet"
    ],
    "supportsOutputJS": true
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "overrides": [
      {
        "files": [
          "**/*.ts"
        ],
        "rules": {
          "@typescript-eslint/explicit-member-accessibility": "off"
        }
      }
    ]
  }
}