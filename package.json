{
  "name": "number-compare",
  "version": "1.0.0-rc.1",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/number-compare.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
